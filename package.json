    "android": "194",
    "ios": "1.26.1",
    "force-update": true,
    "promotion": {
      "android": "200",
      "ios": "1.28.0"
    }
  },
  "main": "index.js",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:webpack": "webpack-dev-server --no-client-overlay --progress profile --mode development",
    "build": "webpack --mode=production",
    "lint": "eslint . --cache --fix",
    "lint:ci": "eslint .",
    "test": "jest",
    "pretest:jest-coverage": "rm -R www/test/* || true",
    "test:jest-coverage": "jest --coverage --colors --ci",
    "test:cy-open": "cypress open",
    "pretest:cy-basic": "rm -R test/cypress/results/* || rm -R test/cypress/screenshots/* || rm -R test/cypress/videos/* || true",
    "pretest:cy-basic-record": "rm -R test/cypress/results/* || rm -R test/cypress/screenshots/* || rm -R test/cypress/videos/* || true",
    "test:cy-basic": "cypress run --browser chrome --spec 'test/cypress/e2e/**/basic*'",
    "test:cy-basic-record": "cypress run --browser chrome --spec 'test/cypress/e2e/**/basic*' --record",
    "test:cy-all": "cypress run --browser chrome --spec 'test/cypress/e2e/**/*'",
    "test:cy-all-record": "cypress run --browser chrome --spec 'test/cypress/e2e/**/*' --record",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint . --cache --fix",
      "git add"
    ]
  },
